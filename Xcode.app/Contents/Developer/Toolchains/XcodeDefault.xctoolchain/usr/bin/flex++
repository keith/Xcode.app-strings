/**< 1-dim ints */
YYTD_ID_BASE = 0x02,
/**< 1-dim ints */
YYTD_ID_CHK = 0x03,
/**< 1-dim ints */
YYTD_ID_DEF = 0x04,
/**< 1-dim ints */
YYTD_ID_EC = 0x05,
/**< 1-dim ints */
YYTD_ID_META = 0x06,
/**< 1-dim ints */
YYTD_ID_NUL_TRANS = 0x07,
/**< 1-dim ints, maybe indices */
